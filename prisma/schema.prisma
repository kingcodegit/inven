generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model SmaSettings {
  setting_id           Int       @id
  logo                 String    @db.VarChar(255)
  logo2                String?    @db.VarChar(255)
  site_name            String    @db.VarChar(55)
  
  @@map("sma_settings")
}